    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "glob": "^10.3.10",
    "prettier": "^3.0.3",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-mv": "^0.0.2",
    "typescript": "^5.1.6",
